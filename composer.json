{
    "name": "rmcgirr83/whoposted",
    "type": "phpbb-extension",
    "description": "Displays a modal window of who posted within a topic in a forum.  Link to the modal is the replies count of the topics.",
    "version": "1.1.4",
    "time": "2021-01-31",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Richard McGirr",
            "homepage": "https://www.phpbb.com/customise/db/author/rmcgirr83/contributions",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpbb/phpbb": ">=3.2",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Who Posted in this topic",
        "soft-require": {
            "phpbb/phpbb": ">=3.2"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/who_posted_in_this_topic",
            "filename": "version_check",
            "ssl": true
        }
    }
}